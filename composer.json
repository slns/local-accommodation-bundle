{
    "name": "slns/local-accommodation-bundle",
    "description": "A reusable local accommodation management bundle for Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LocalAccommodationBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^7.0 || ^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/console": "^7.3",
        "doctrine/orm": "^3.5",
        "doctrine/doctrine-bundle": "^2.15"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0"
        }
    }
}
